{
  "name": "interaccounts-contract",
  "version": "0.1.0",
  "private": true,
  "description": "Agoric contract for interchain accounts (ICS-27)",
  "type": "module",
  "scripts": {
    "build": "exit 0",
    "test": "npx ava ./test/test-contract.js",
    "lint": "npx eslint '**/*.{js,jsx}'",
    "lint-fix": "npx eslint --fix '**/*.{js,jsx}'",
    "lint-check": "npx eslint '**/*.{js,jsx}'",
    "lint-fix-jessie": "npx eslint -c '.eslintrc-jessie.js' --fix '**/*.{js,jsx}'",
    "lint-check-jessie": "npx eslint -c '.eslintrc-jessie.js' '**/*.{js,jsx}'"
  },
  "devDependencies": {
    "@agoric/babel-standalone": "^7.17.7",
    "ava": "^3.13.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-jessie": "^0.0.6",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "^2.1.2"
  },
  "dependencies": {
    "@agoric/assert": "dev",
    "@agoric/ertp": "dev",
    "@agoric/notifier": "dev",
    "@agoric/store": "dev",
    "@agoric/swingset-vat": "dev",
    "@agoric/zoe": "dev",
    "@cosmjs/encoding": "^0.28.4",
    "@endo/base64": "^0.2.25",
    "@endo/bundle-source": "^2.2.0",
    "@endo/eventual-send": "^0.15.3",
    "@endo/init": "^0.5.41",
    "@endo/marshal": "^0.6.7",
    "@endo/promise-kit": "^0.2.42",
    "cosmjs-types": "agoric-labs/cosmjs-types#Agoric-built"
  },
  "resolutions": {
    "**/protobufjs": "agoric-labs/protobuf.js#Agoric-built"
  },
  "ava": {
    "files": [
      "test/**/test-*.js"
    ],
    "timeout": "10m"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pital/interaccounts"
  },
  "author": "Pital",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/pital/interaccounts/issues"
  },
  "homepage": "https://github.com/pital/interaccounts#readme",
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "overrides": [
      {
        "files": [
          "*.js",
          "*.cjs",
          "*.mjs"
        ]
      }
    ]
  }
}
